{
  "name": "sam-vue",
  "version": "1.0.0",
  "description": "Self-Assessment Manikin (SAM) Vue component library",
  "type": "module",
  "main": "./dist/sam-vue.umd.js",
  "module": "./dist/sam-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sam-vue.es.js",
      "require": "./dist/sam-vue.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "images"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/build-types.js",
    "build:demo": "vite build --config vite.config.demo.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "vue",
    "sam",
    "self-assessment-manikin",
    "emotion",
    "psychology"
  ],
  "author": "SAM.vue contributors",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.1.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.17",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  }
}
