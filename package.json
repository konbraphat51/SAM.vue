{
  "name": "@konbraphat51/samvue",
  "version": "1.1.1",
  "description": "Self-Assessment Manikin (SAM) Vue component library",
  "type": "module",
  "main": "./dist/sam-vue.umd.js",
  "module": "./dist/sam-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sam-vue.es.js",
      "require": "./dist/sam-vue.umd.js"
    }
  },
  "files": [
    "dist",
    "images"
  ],
  "scripts": {
    "build": "vite build && node scripts/build-types.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "vue",
    "sam",
    "self-assessment-manikin",
    "emotion",
    "psychology"
  ],
  "author": "SAM.vue contributors",
  "license": "BSL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/konbraphat51/SAM.vue"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.1.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vitest": "^4.0.17",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  }
}
